# Firecrawl API Configuration
FIRECRAWL_API_KEY=fc-your-firecrawl-api-key-here
FIRECRAWL_API_BASE_URL=https://api.firecrawl.dev/v2/x402/search

# x402 Payment Configuration
# Note: When using CDP SDK, you don't need to manually set wallet address/private key
# The CDP SDK manages wallets automatically
# Optional: Manual wallet configuration (not needed when using CDP SDK)
# WALLET_ADDRESS=0xYourWalletAddressHere
# X402_PRIVATE_KEY=your-private-key-here

# x402 Facilitator URL
FACILITATOR_URL=https://api.x402.org/facilitator

# Network (base for mainnet - Firecrawl requires mainnet)
NEXT_PUBLIC_NETWORK=base

# USDC Contract Address for Base network
USDC_CONTRACT_ADDRESS=0x833589fCD6eDb6E08f4c7C32D4f71b54bdA02913

# CDP Configuration
NEXT_PUBLIC_CDP_PROJECT_ID=your-cdp-project-id-here

CDP_API_KEY_ID=your-cdp-api-key-id
CDP_API_KEY_SECRET=your-cdp-api-key-secret
